    "@sveltejs/adapter-vercel": "^5.5.2",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "ai": "4.2.0",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^1.3.9",
    "clsx": "^2.1.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
