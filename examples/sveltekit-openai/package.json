{
  "name": "sveltekit-openai",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "type-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
