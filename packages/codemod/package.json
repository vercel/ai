    "@types/node": "20.17.24",
    "@types/semver": "^7.5.8",
    "@vercel/ai-tsconfig": "workspace:*",
    "msw": "2.6.4",
    "tsup": "^8",
    "tsx": "4.19.2",
    "typescript": "5.8.3",
    "vitest": "2.1.4"
  },
  "bin": {
    "codemod": "./dist/bin/codemod.js"
  },
  "engines": {
    "node": ">=18"
  },
