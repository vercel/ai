{
  "name": "@ai-sdk/svelte",
  "version": "4.0.0-beta.98",
  "license": "Apache-2.0",
  "scripts": {
    "build": "pnpm prepack",
    "build:watch": "pnpm clean && pnpm --filter svelte build && svelte-package --input=src --watch",
    "preview": "vite preview",
    "prepare": "echo ''",
    "prepack": "pnpm clean && svelte-package --input=src && publint",
    "type-check": "svelte-check --tsconfig ./tsconfig.json",
    "type-check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "prettier-fix": "prettier --write .",
    "prettier-check": "prettier --check .",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:update": "vitest --run -u",
    "test:watch": "vitest",
    "clean": "del-cli dist *.tsbuildinfo"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "src",
    "!src/**/*.test.*",
    "!src/**/*.spec.*",
    "CHANGELOG.md",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "svelte": "^5.31.0"
  },
  "dependencies": {
    "@ai-sdk/provider-utils": "workspace:*",
    "ai": "workspace:*"
  },
  "devDependencies": {
    "@ai-sdk/test-server": "workspace:*",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@types/node": "20.17.24",
    "@vercel/ai-tsconfig": "workspace:*",
    "eslint": "^9.18.0",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "publint": "^0.3.2",
    "svelte": "^5.31.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "zod": "3.25.76"
  },
  "homepage": "https://ai-sdk.dev/docs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel/ai.git",
    "directory": "packages/svelte"
  },
  "bugs": {
    "url": "https://github.com/vercel/ai/issues"
  },
  "keywords": [
    "ai",
    "svelte"
  ]
}
